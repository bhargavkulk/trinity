fun start()
{
    var a: int[3, 4];
    
    var i: int = 0;
    var j: int = 0;
    while i < 3
    {
        j = 0;
        while j < 4
        {
            a[i, j] = i + j;
            j = j + 1;
        }
        i = i + 1;
    }

    i = 0;
    while i < 3
    {
        j = 0;
        while j < 4
        {
            log a[i, j];
            j = j + 1;
        }
        log "";
        i = i + 1;
    }
}
